{
  "name": "fluttergpt",
  "displayName": "CommandDash: Your Flutter AI Autopilot with Gemini",
  "description": "Agents that assist you build entire features on autopilot.",
  "version": "0.3.5",
  "publisher": "WelltestedAI",
  "icon": "media/icon.png",
  "engines": {
    "vscode": "^1.82.0"
  },
  "repository": {
    "url": "https://github.com/Welltested-AI/dash-ai"
  },
  "bugs": {
    "url": "https://github.com/Welltested-AI/dash-ai/issues",
    "email": "team@commanddash.io"
  },
  "homepage": "https://commanddash.io",
  "categories": [
    "Programming Languages",
    "Machine Learning",
    "Testing"
  ],
  "keywords": [
    "flutter",
    "dart",
    "copilot",
    "gpt",
    "dash",
    "ai",
    "chat",
    "coding",
    "commandash",
    "command"
  ],
  "activationEvents": [
    "onLanguage:dart"
  ],
  "extensionDependencies": [
    "Dart-Code.dart-code"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "extensionKind": [
      "ui"
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "webview",
          "title": "CommandDash",
          "icon": "./media/activity-icon.png"
        }
      ]
    },
    "views": {
      "webview": [
        {
          "type": "webview",
          "id": "dash.chatView",
          "name": "Panel"
        }
      ]
    },
    "configuration": {
      "title": "CommandDash",
      "type": "object",
      "properties": {
        "fluttergpt.apiKey": {
          "type": "string",
          "markdownDescription": "Gemini API KEY(https://makersuite.google.com/)"
        }
      }
    },
    "keybindings": [
      {
        "command": "dash.attachToDash.menu",
        "key": "ctrl+R",
        "mac": "cmd+R"
      },
      {
        "command": "dash.createInlineCodeCompletion.menu",
        "key": "ctrl+shift+right",
        "mac": "cmd+shift+right",
        "when": "!editorHasSelection && editorTextFocus && resourceLangId == 'dart'"
      }
    ],
    "commands": [
      {
        "command": "dash.attachToDash",
        "title": "Dash: Attach Snippet to Dash"
      },
      {
        "command": "dash.attachToDash.menu",
        "title": "Attach Snippet to Dash"
      },
      {
        "command": "dash.createInlineCodeCompletion",
        "title": "Dash: Inline Code Generation"
      },
      {
        "command": "dash.createInlineCodeCompletion.menu",
        "title": "Inline Code Generation"
      },
      {
        "command": "dash.createWidget",
        "title": "Dash Create: Widget from Image or Description"
      },
      {
        "command": "dash.createWidget.menu",
        "title": "Widget from Image or Description"
      },
      {
        "command": "dash.createCodeFromBlueprint",
        "title": "Dash Create: Code from Blueprint"
      },
      {
        "command": "dash.createCodeFromBlueprint.menu",
        "title": "Code from Blueprint"
      },
      {
        "command": "dash.createCodeFromDescription",
        "title": "Dash Create: Code From Description"
      },
      {
        "command": "dash.createCodeFromDescription.menu",
        "title": "Code From Description"
      },
      {
        "command": "dash.fixErrors",
        "title": "Dash Refactor: Fix Errors"
      },
      {
        "command": "dash.optimizeCode",
        "title": "Dash Refactor: Optimize"
      },
      {
        "command": "dash.clearChat",
        "title": "Clear Chat",
        "icon": {
          "light": "resources/light/trash.svg",
          "dark": "resources/dark/trash.svg"
        }
      },
      {
        "command": "dash.marketPlace",
        "title": "Market Place",
        "icon": {
          "light": "resources/light/market-place.svg",
          "dark": "resources/dark/market-place.svg"
        }
      },
      {
        "command": "dash.backButton",
        "title": "Back",
        "icon": {
          "light": "resources/light/back.svg",
          "dark": "resources/dark/back.svg"
        }
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "dash.attachToDash",
          "when": "editorHasSelection && resourceLangId == 'dart'"
        },
        {
          "command": "dash.attachToDash.menu",
          "when": "false"
        },
        {
          "command": "dash.createInlineCodeCompletion",
          "when": "!editorHasSelection && editorTextFocus && resourceLangId == 'dart'"
        },
        {
          "command": "dash.createInlineCodeCompletion.menu",
          "when": "false"
        },
        {
          "command": "dash.createWidget",
          "when": "!editorHasSelection && resourceLangId == 'dart'"
        },
        {
          "command": "dash.createWidget.menu",
          "when": "false"
        },
        {
          "command": "dash.createCodeFromBlueprint",
          "when": "editorHasSelection && resourceLangId == 'dart'"
        },
        {
          "command": "dash.createCodeFromBlueprint.menu",
          "when": "false"
        },
        {
          "command": "dash.createCodeFromDescription",
          "when": "!editorHasSelection && resourceLangId == 'dart'"
        },
        {
          "command": "dash.createCodeFromDescription.menu",
          "when": "false"
        },
        {
          "command": "dash.fixErrors",
          "when": "editorHasSelection && resourceLangId == 'dart'"
        },
        {
          "command": "dash.optimizeCode",
          "when": "editorHasSelection && resourceLangId == 'dart'"
        }
      ],
      "editor/context": [
        {
          "submenu": "dash.createMenu",
          "group": "dash-menu-group@1"
        },
        {
          "command": "dash.createInlineCodeCompletion.menu",
          "when": "!editorHasSelection && editorTextFocus && resourceLangId == 'dart'",
          "group": "dash-menu-group@2"
        },
        {
          "command": "dash.attachToDash.menu",
          "when": "editorHasSelection && resourceLangId == 'dart'",
          "group": "dash-menu-group@3"
        }
      ],
      "view/title": [
        {
          "command": "dash.clearChat",
          "group": "navigation",
          "when": "view == dash.chatView && !dash:isMarketPlace"
        },
        {
          "command": "dash.marketPlace",
          "group": "navigation",
          "when": "view == dash.chatView && !dash:isMarketPlace"
        },
        {
          "command": "dash.backButton",
          "group": "navigation",
          "when": "view == dash.chatView && dash:isMarketPlace"
        }
      ],
      "dash.createMenu": [
        {
          "command": "dash.createCodeFromBlueprint.menu",
          "when": "editorHasSelection && resourceLangId == 'dart'",
          "group": "dash-menu-group@1"
        },
        {
          "command": "dash.createCodeFromDescription.menu",
          "when": "!editorHasSelection && resourceLangId == 'dart'",
          "group": "dash-menu-group@2"
        },
        {
          "command": "dash.createWidget.menu",
          "when": "!editorHasSelection && resourceLangId == 'dart'",
          "group": "dash-menu-group@3"
        }
      ]
    },
    "submenus": [
      {
        "id": "dash.createMenu",
        "label": "CommandDash: Create"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run -S esbuild-base",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "build": "npm run -S esbuild-base -- --sourcemap",
    "watch": "npm run -S esbuild-base -- --sourcemap --watch",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vscode/test-electron": "^2.3.0",
    "esbuild": "^0.20.1",
    "eslint": "^8.39.0",
    "glob": "^10.2.2",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@google/generative-ai": "^0.1.1",
    "@vscode/extension-telemetry": "^0.8.1",
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "js-yaml": "^4.1.0",
    "tailwindcss": "^3.4.3",
    "url-metadata": "^3.3.1",
    "vscode-languageclient": "^9.0.1"
  }
}
